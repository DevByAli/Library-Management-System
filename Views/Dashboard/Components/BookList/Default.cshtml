@using LMS
@model List<Book>

<html>
    <head>
        <title>Dashboard</title>
        <link rel="stylesheet" href="/css/home.css">
    </head>
    <body>
        @if(@Model.Count==0)
        {
            <h1>No Record!</h1>
        }
        else
        {
            <div class="table-div">
                <table>
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Author</th>
                            <th>IBAN</th>
                        </tr>
                    </thead>
                    <partial name="_MyPartialView" model="Model"/>
                </table>
            </div>
        }
</body>
</html>